{
    "name": "cledilsonweb/lontra-validator",
    "type": "library",
    "description": "PHP data validators and extra validators for Laminas Framework(getlaminas.org)",
    "keywords": [
        "cledilsonweb",
        "lontra-validator",
        "laminas",
        "validator"
    ],
    "version": "0.1.0",
    "homepage": "https://github.com/cledilsonweb/lontra-validator",
    "license": "MIT",
    "authors": [{
        "name": "Cledilson Nascimento",
        "email": "contato@cledilsonweb.com.br",
        "homepage": "https://github.com/cledilsonweb",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.2",
        "laminas/laminas-validator": "^2.13"
    },
    "require-dev": {
        "laminas/laminas-db": "^2.13",
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "suggest": {
        "laminas/laminas-db": "Laminas\\Db component, required by validators with database access"
    },
    "autoload": {
        "psr-4": {
            "Lontra\\Validator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lontra\\ValidatorTest\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
